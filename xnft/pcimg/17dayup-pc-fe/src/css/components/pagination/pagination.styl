.w-pagination
    margin-top 60px
    margin-bottom 60px
    a
        display inline-block
        margin-right 6px
        border-radius 4px
        font-size 16px
        height 42px
        line-height 42px
        min-width 42px
        text-align center
    .head
    .tail
        padding 0 14px
    .w-active-page
        background #238FFF
        color #fff
        border 1px solid #238FFF
        &:hover
            color #fff
    .page-init-sty
        background #fff
        border 1px solid #CCCCCC
        color #9B9B9B
    .w-page-disabled
        cursor  not-allowed
        color #fff
        background #ccc
        -webkit-box-shadow none
        box-shadow none
        border 1px solid #ccc
        opacity .65
.my-pagination
    text-align center
    margin 0px auto 35px
